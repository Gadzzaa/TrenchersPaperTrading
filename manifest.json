{
  "name": "TrenchersPaperTrading",
  "description": "An application to simulate paper trading.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
   {
     "resources": ["index.html"],
     "matches": ["<all_urls>"]
   }
  ],
  "js": [
        "Scripts/index.js",
        "Scripts/account.js"
  ],
  "css": [
    "Styles/index.css",
    "Styles/account.css"
  ]
}
