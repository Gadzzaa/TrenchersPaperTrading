
{
  "name": "TrenchersPaperTrading",
  "description": "An application to simulate paper trading.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://axiom.trade/meme/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://axiom.trade/meme/*"],
      "js": ["Scripts/inject.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "account.html",
        "dashboard.html",
        "Styles/index.css",
        "Styles/account.css"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
